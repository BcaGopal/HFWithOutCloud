<script type="text/javascript">


    $(function () {

        CustomSelectFunction($(".ProdHelpList"), '/ComboHelpList/GetProducts', '/ComboHelpList/SetSingleProducts', 'Enter Product', false, 3);
        CustomSelectFunction($(".Dime1HelpList"), '/ComboHelpList/GetDimension1', '/ComboHelpList/SetSingleDimension1', 'Enter Dimension1', false, 3);
        CustomSelectFunction($(".Dime2HelpList"), '/ComboHelpList/GetDimension2', '/ComboHelpList/SetSingleDimension2', 'Enter Dimension2', false, 3);


        $(document).on('change', '.ProdHelpList,.Dime1HelpList,.Dime2HelpList', function () {
            var $Block = $(this).closest('div.row.crow');
            GetExcessStock($Block);
        })


        $('div.row.crow', '.modal-body').each(function () {
            GetExcessStock(this);
        });
    });

    function GetExcessStock(obj) {
        var Lock = false;
        var Error = false;
        if ($(obj).find('span.ExcessStockQty')) {

            var ProcName = '@Model.StockHeaderSettings.SqlFuncCurrentStock';
            var $Block = $(obj)
            var $this = $(obj).find('span.ExcessStockQty');
            if (!Error) {
                $.get('/StockIssueLine/GetExcessStock', { ProductId: parseInt($('input.ProdId', $Block).val()), Dim1: parseInt($('input.Dim1Id', $Block).val()), Dim2: parseInt($('input.Dim2Id', $Block).val()), ProcId: null, Lot: $('input.LotNo', $Block).val(), MaterialIssueId: parseInt($('input.StockHeaderId', $Block).val()), ProcName: ProcName }).done(function (data) {
                    $this.text("Stock:" + data);
                }).error(function () {
                    Lock: true;
                });
            }
        }
    };

    var reset = false;
    var IsSaved = false;
    var ValidationCount = 0;

    $('#modform').bind('submit', function (event) {

        if (!IsSaved) {
            if (!ValidateData()) {
                return false;
            }
            IsSaved = true;
            $('button:submit').attr('disabled', 'disabled');
            return IsSaved;
        }
        else {
            return false;
        }


    });

    function ValidateData() {
        var ValidationMessage = "";

        //Client Side Validaion.....

        return (ValidationMessage == "");

    }


    $('input.IssueQty').change(function () {


        var Issue = 0;
        var maxDeclaces = 0;
        var ValidationMessage = "";
        var ValidationMessageCount = 0;

        $('.row.crow.grid-body', '.modal-body').each(function () {
            var $Input = $(this).find('input.IssueQty');

            if ($Input.val()) {
                Issue += $Input.hasClass('number') ? parseFloat(eval($Input.val())) : parseFloat($Input.val());
                var $Hidiv = $(this).closest('div.row.crow');
                var unitDecPlaces = parseFloat($Hidiv.find('.UDec').val()) || 0;
                if (unitDecPlaces > maxDeclaces)
                    maxDeclaces = unitDecPlaces;
            }

            var ReqBalQty = parseFloat($(this).find('#StockInQty').text()) || 0;
            var LineIssueQty = $Input.hasClass('number') ? eval(parseFloat($Input.val())) : parseFloat($Input.val());

            if (LineIssueQty > ReqBalQty) {
                $(this).addClass('ModalRowValidationError')
                ValidationMessage = "Qty Exceeding Balance Qty";
                ValidationMessageCount++;
            }
            else {
                if ($(this).hasClass('ModalRowValidationError'))
                    $(this).removeClass('ModalRowValidationError');
            }
        })

        $('#IssueQty', '.modal-body').text(Issue.toFixed(maxDeclaces));

        var $div = $(this).closest('div.row');
        var StockInQty = parseFloat($div.find('#StockInQty').text()) || 0;

        var IssueQty = parseFloat($div.find('input.IssueQty').val()) || 0;

        var bal = (IssueQty) - (StockInQty);

        if (bal > 0) {
            if (!$('#modform').find('.modal-body #errormsg').length) {
                $('#modform').find('.modal-body').prepend("<div class='alert alert-danger'><span id=errormsg >" + ValidationMessage + "</span></div>")
            }
            else {
                $('#modform').find('#errormsg').text("Qty exceeding Required Qty");
            }
        }
        else {
            if ($('#modform').find('.modal-body #errormsg').length && ValidationMessageCount == 0) {
                $('#modform').find('#errormsg').parent().remove();
            }
        }
        //CalculateTotal();

    })

    function CalculateTotal() {
        var Receipt = 0;
        var maxDeclaces = 0;
        $('.row.crow.grid-body', '.modal-body').each(function () {
            var $Input = $(this).find('input.IssueQty');
            if ($Input.val()) {
                Receipt += $Input.hasClass('number') ? parseFloat(eval($Input.val())) : parseFloat($Input.val());
                var $Hidiv = $(this).closest('div.row.crow');
                var unitDecPlaces = parseFloat($Hidiv.find('.UDec').val()) || 0;
                if (unitDecPlaces > maxDeclaces)
                    maxDeclaces = unitDecPlaces;
            }
        })
        $('#IssueQty', '.modal-body').text(Receipt.toFixed(maxDeclaces));
    }

    $('.ToIssue').click(function () {

        var $Hidiv = $(this).closest('div.row.crow');

        var unitDecPlaces = parseFloat($Hidiv.find('.UDec').val()) || 0;

        var $div = $(this).closest('div.row');

        var IssueQty = parseFloat($div.find('#StockInQty').text()) || 0;

        if ($div.find('input.IssueQty').val() && $div.find('input.IssueQty').val() > 0) {
            $div.find('input.IssueQty').val(0);
            $div.find('input.ReceiptDealQty').val(0);
        }

        else {
            $div.find('input.IssueQty').val(IssueQty.toFixed(unitDecPlaces));
        }

        CalculateTotal();

    })

    $('#ToIssue').click(function () {
        if (reset) {
            $('div.row.crow', "#gbody.panel-info").each(function (index) {


                var $Hidiv = $(this);

                var unitDecPlaces = parseFloat($Hidiv.find('.UDec').val()) || 0;

                var IssueQty = parseFloat($Hidiv.find('#StockInQty').text()) || 0;

                $Hidiv.find('input.IssueQty').val(IssueQty.toFixed(unitDecPlaces));

            })
            CalculateTotal();
            reset = false;
        }
        else if (!reset) {
            $('div.row.crow', "#gbody.panel-info").each(function (index) {

                var $Hidiv = $(this);

                $Hidiv.find('input.IssueQty').val(0);

            })
            CalculateTotal();
            reset = true;
        }
    })



