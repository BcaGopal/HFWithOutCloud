<script type="text/javascript">
    function Expand_Detail(Summary_DivId, Detail_DivId) {
        $(Summary_DivId).click(function () {
            var IsVisibleDiv = $(Detail_DivId).is(":visible");
            $(Detail_DivId).slideToggle();

            if (IsVisibleDiv == false)
            {
                $('html, body').animate({
                    //scrollTop: $(Detail_DivId).offset().top
                    'scrollTop' : $(Detail_DivId).position().top
                }, 1000);
            }
        });
    }

    Expand_Detail('#SaleOrderSummary', "#SaleOrderDetail")
    Expand_Detail('#SaleSummary', "#SaleDetail")
    Expand_Detail('#PurchaseSummary', "#PurchaseDetail")
    Expand_Detail('#PackingSummary', "#PackingDetail")

    Expand_Detail('#SaleOrderBalanceSummary', "#SaleOrderBalanceDetail")
    Expand_Detail('#PackedButNotShippedSummary', "#PackedButNotShippedDetail")
    Expand_Detail('#JobOrderBalanceSummary', "#JobOrderBalanceDetail")
    Expand_Detail('#ProcessReceiveSummary', "#ProcessReceiveDetail")

    Expand_Detail('#SaleOrderDetailClose', "#SaleOrderDetail")
    Expand_Detail('#SaleDetailClose', "#SaleDetail")
    Expand_Detail('#PurchaseDetailClose', "#PurchaseDetail")
    Expand_Detail('#PackingDetailClose', "#PackingDetail")

    Expand_Detail('#SaleOrderBalanceDetailClose', "#SaleOrderBalanceDetail")
    Expand_Detail('#PackedButNotShippedDetailClose', "#PackedButNotShippedDetail")
    Expand_Detail('#JobOrderBalanceDetailClose', "#JobOrderBalanceDetail")
    Expand_Detail('#ProcessReceiveDetailClose', "#ProcessReceiveDetail")
</script>

