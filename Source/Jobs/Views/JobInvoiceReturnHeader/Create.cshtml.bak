<script type="text/javascript">
    var form_clean;
    $(document).ready(function () {

        //Calling the function to load the table data/ Line information
        LoadProducts();

        //Adding Hidden Input Fields
        AddFields();


        //Adding Charges on Doc Load
        var id=@Model.JobInvoiceReturnHeaderId;

        LoadCharges(id,"Web.JobInvoiceReturnHeaderCharges","Web.JobInvoiceReturnLineCharges","/TaxCalculation/GetJIRHeaderChargeForEdit");
    });


    function LoadProducts() {
        var DomainName='@System.Configuration.ConfigurationManager.AppSettings["AdminSetupDomain"]';
        var groupId=@Model.JobInvoiceReturnHeaderId;
        var status='@Model.Status';
        var LineEditUrl;
        var LineDeleteUrl;
        if(status==StatusContstantsEnum.Drafted)
        {
            LineEditUrl="/JobInvoiceReturnLine/_ModifyLine/";
            LineDeleteUrl="/JobInvoiceReturnLine/_DeleteLine/";
        }
        else if(status==StatusContstantsEnum.Submitted || status == StatusContstantsEnum.Modified)
        {
            LineEditUrl="/JobInvoiceReturnLine/_ModifyLineAfterSubmit/";
            LineDeleteUrl="/JobInvoiceReturnLine/_DeleteLine_AfterSubmit/";
        }
        else {
            LineEditUrl = "/JobInvoiceReturnLine/_Detail/";
            LineDeleteUrl = "/JobInvoiceReturnLine/_Detail/";
        }
        var link="/JobInvoiceReturnLine/Index/"+groupId;
        var QtyCount=0;
        var DealQtyCount=0;
        var AmountCount=0;
        var unit=true;
        var dunit=true;
        var prevunit;
        var prevdunit;
        var decimal=0;
        var ddecimal=0;
        $.ajax({
            url: link,
            type: "GET",
            success: function (data) {
                var row = ""
                var srno=1;

                if(data.length!=0)
                {
                    $.each(data, function (index, item) {
                        
                        var ReceiptUrl=DomainName+"/Redirect/RedirectToDocument?DocTypeId="+item.JobInvoiceDocTypeId+"&DocId="+item.JobInvoiceHeaderId+"&DocLineId="+item.ReceiptLineId;
                        var BarCodeUrl=DomainName+"/Redirect/BarCodeDetail?BarCode="+item.ProductUidName;

                        row += " <div class='row crow grid-body' style='margin-left:0px;margin-right:0px'>"
                            +"      <div class='row-index block'  style='width:5%'><a Detail='' data-modal href='/JobInvoiceReturnLine/_Detail/"+item.JobInvoiceReturnLineId+"'>"+srno+"</a><input type='hidden' class='id' value='"+item.JobInvoiceReturnLineId+"' /></div>"
                            +"          <div class='block'  style='width:90%'>"
                            +"              <div class='row'>"
                            +"                  <div class='block' style='width:25%'>"+(item.ProductName==null?" ":"<strong>"+item.ProductName+"</strong>")
                            +"                      <br />"+(item.Specification==null?" ":item.Specification)
                            +"                          <div>"+(item.Dimension1Name==null?" ":item.Dimension1Name+",")+(item.Dimension2Name==null?" ":item.Dimension2Name)+(item.Dimension3Name==null?" ":item.Dimension3Name)+(item.Dimension4Name==null?" ":item.Dimension4Name)+"</div>"
                            +"                          <div>"+((item.DueDate==null)?" ":"<span title='DueDate' class='label label-danger' style='font-weight:bold'>"+formatDate('d/m/Y',new Date(parseInt(item.DueDate.substr(6))))+"</span>")
                            +"                          </div>"
                            +"                           <div>"+(item.ProductUidName==null?" ":"Product UID:<a href='"+BarCodeUrl+"' target='_blank'> "+item.ProductUidName+"</a>")+"</div>"
                            +              (item.Remark==null?" ":item.Remark)
                            +"                  </div>"
                            +"                  <div class='block'  style='width:15%'>"+(item.JobInvoiceHeaderDocNo==null?" ":"<a href='"+ReceiptUrl+"' style='color:black'><span title='DueDate' class='label label-danger' style='font-weight:bold'>"+item.JobInvoiceHeaderDocNo)+"</span></a><br />"
                            +"                      <div>"+(item.JobReceiveHeaderDocNo==null?" ":item.JobReceiveHeaderDocNo)
                            +"                      </div>"
                            +"                   </div>"
                            +"                  <div class='text-right block'  style='width:10%'>"+(item.Qty==null?" ":item.Qty.toFixed(item.unitDecimalPlaces))+"<br />"
                            +"                      <div><small>"+(item.UnitName==null?" ":item.UnitName)+"</small>"
                            +"                      </div>"
                            +"                   </div>"
                            +"                  <div class='text-right block'  style='width:10%'>"+(item.DealQty==null?" ":item.DealQty.toFixed(item.DealunitDecimalPlaces))+"<br />"
                            +"                      <div><small>"+(item.DealUnitName==null?" ":item.DealUnitName)+"</small>"
                            +"                      </div>"
                            +"                   </div>"
                            +"                   <div class='text-right block'  style='width:10%'>"+(item.Rate==null?" ":item.Rate.toFixed(2))
                            +"                   </div>"
                            +"                  <div class='col-sm-2 text-right'>"+(item.DiscountPer==null?" ":item.DiscountPer.toFixed(2))
                            +"                  </div>"
                            +"                   <div class=' text-right block'  style='width:10%'>"+(item.Amount==null?" ":item.Amount.toFixed(2))
                            +"                   </div>"
                            +"              </div>"
                            +"          </div>"
                            +"      <div class='block'  style='width:5%'><a class='glyphicon glyphicon-trash icon-Cus-Size' data-modal data-detailDisabled='true' delete='' href='"+LineDeleteUrl+item.JobInvoiceReturnLineId+"'></a><a  edit='' href='"+LineEditUrl+item.JobInvoiceReturnLineId+"'></a>"
                            +"      </div>"
                            +"  </div>"
                            +"<hr style='margin-top:0px;margin-bottom:0px'/>";
                        QtyCount+=item.Qty;
                        DealQtyCount+=item.DealQty;
                        AmountCount+=item.Amount;
                        if(srno!=1)
                        {
                            if(item.UnitName==prevunit && unit!=false)
                            {}
                            else
                            {unit=false;}
                            if(item.DealUnitName==prevdunit && dunit!=false)
                            {}
                            else
                            {dunit=false;}
                        }

                        if(decimal<item.unitDecimalPlaces){decimal=item.unitDecimalPlaces;}
                        if(ddecimal<item.DealunitDecimalPlaces){ddecimal=item.DealunitDecimalPlaces;}
                        srno++;
                        prevunit=item.UnitName;
                        prevdunit=item.DealUnitName;
                    });
                    row+="<div class='row tfooter' style='padding-top:10px;'>"
                        +"  <div class='block' style='width:5%'>"
                        +"  </div>"
                        +"  <div class='block' style='width:90%'>"
                        +"      <div class='row'> "
                        +"          <div class='block'  style='width:25%'><strong>Total</strong></div>"
                        +"          <div class='block' style='width:15%'>"
                        +"          </div>"
                        +"          <div class='block text-right' style='width:10%'><strong>"
                        +               ((unit==true)?QtyCount.toFixed(decimal)+"<br /><small>"+prevunit:QtyCount.toFixed(decimal))+"</small></strong>"
                        +"          </div>"
                        +"          <div class='block text-right' style='width:10%'><strong>"
                        +               ((dunit==true)?DealQtyCount.toFixed(ddecimal)+"<br /><small>"+prevdunit:DealQtyCount.toFixed(ddecimal))+"</small></strong>"
                        +"          </div>"
                        +"          <div class='block text-right' style='width:10%'>"
                        +"          </div>"
                        +"                  <div class='col-sm-2 text-right'>"
                        +"                  </div>"
                        +"          <div class='block text-right' style='width:10%'><strong>"+AmountCount.toFixed(2)+"</strong>"
                        +"          </div>"

                        +"      </div>"
                        +"  </div>"
                        +"  <div class='block'  style='width:5%'>"
                        +"  </div>"
                        +"</div>"
                }
                else
                {
                    ;
                }
                $("#gbody").html(row);

            },
            error: function (result) {
                alert("Error");
            }
        });
    }




</script>


