                    @Html.ActionLink(" ", "GeneratePrints", "JobInvoiceReturnHeader", new { Ids = Model.JobInvoiceReturnHeaderId, DocTypeId = Model.DocTypeId }, new { @class = "glyphicon glyphicon-print toolbar", @title = "Print", @data_toggle = "tooltip", @id = "PrintRecordC", target = "_blank" })
                    @if (Model.Status == (int)StatusConstants.Submitted && !Model.GatePassHeaderId.HasValue)
                    {
                        @Html.ActionLink(" ", "GenerateGatePass", "JobInvoiceReturnHeader", new { Ids = @Model.JobInvoiceReturnHeaderId, DocTypeId = Model.DocTypeId }, new { @class = "glyphicon glyphicon-log-out toolbar", @title = "GatePass", @data_toggle = "tooltip", @id = "GenGatePassC" })
                    }
                    @Html.ActionLink(" ", "Email", "JobInvoiceReturnHeader", null, new { @class = "glyphicon glyphicon-envelope toolbar", @title = "Email", @data_toggle = "tooltip" })
