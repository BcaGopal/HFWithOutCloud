                    @Html.ActionLink(" ", "NextPage", "JobInvoiceReturnHeader", new { DocId = @Model.JobInvoiceReturnHeaderId, DocTypeId = Model.DocTypeId }, new { @class = "glyphicon glyphicon-arrow-right toolbar", @title = "Next Record", @data_toggle = "tooltip" })
