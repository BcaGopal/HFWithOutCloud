@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@ViewBag.ProductTypeId


@using (Html.BeginForm("WeavingReceiveImportFromBranchToMain", "WeavingReceiveImportFromBranchToMain", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    
    <input type="hidden" name="id" value="@ViewBag.id" />
    
    <div class="panel panel-default">
        <div class="panel-heading">

            <h3>
                <span style="font-family: Calibri,sans-serif,Verdana;font-weight:bold" class="black"> <span class="glyphicon glyphicon-export black"></span>Import Weaving Bazar From Branch</span>
              
            </h3>

            <hr style="margin-top:0px;margin-bottom:0px" />

            <div class="panel-info">
                <h4 style="padding:5px 0 0 0;margin:0px">
                    @Html.ActionLink(" ", " ", " ", null, new { @class = "glyphicon glyphicon-refresh toolbar", @title = "Refresh Page", @data_toggle = "tooltip", @id = "RefreshPage" })
                    @*@Html.ActionLink(" ", "Index", "JobReceiveHeader", null, new { @class = "glyphicon glyphicon-list toolbar right", @title = "List", @data_toggle = "tooltip" })*@                    
                    @Html.ActionLink(" ", "ReturnToRoute", "WeavingReceiveImportFromBranchToMain", new { @id = ViewBag.id }, new { @class = "glyphicon glyphicon-list toolbar right", @title = "List", @data_toggle = "tooltip" })
                </h4>
            </div>

        </div>
        <div class="panel-info" style="padding-left:15px">
            <div class="form-horizontal">

                
                <h4 class="black">Enter Values:</h4><p>
                    <table class="table table-responsive table-hover" style="max-width:60%">
                        <tr style="border-bottom:thin solid #dbd1d1" class="danger">
                            <td>
                                Barnch Weaving Receive No
                            </td>
                            <td colspan="2">
                                <input class="form-control required" data-val="true" id="JobReceiveHeaderIds" name="JobReceiveHeaderIds" type="text">
                            </td>
                        </tr>



                        <tr style="border-bottom:thin solid #dbd1d1" class="danger">
                            <td>
                                Godown
                            </td>
                            <td colspan="2">
                                <input class="form-control required" data-val="true" id="GodownId" name="GodownId" type="text" value="@(ViewBag.GodownId ?? String.Empty)">
                            </td>
                        </tr>

                        <tr style="border-bottom:thin solid #dbd1d1" class="danger">
                            <td>
                                Job Receive By
                            </td>
                            <td colspan="2">
                                <input class="form-control required" data-val="true" id="JobReceiveById" name="JobReceiveById" type="text">
                            </td>
                        </tr>
                    </table>
                </p>

                <div class="form-group">
                    <div class="col-md-10">
                        <div class="row">
                            <div class="col-md-8">
                                <input type="submit" value="Upload" class="btn btn-primary fileinput-button" id="submit" style="width: 90px; background-color: #2980b9" />
                            </div>
                        </div>
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    </div>
                </div>

                @*<div class="form-group">
                    <div class="col-md-offset-1 col-md-10">
                        <input type="submit" value="Upload" class="btn btn-primary btn-sm CurveBorder" />
                        @Html.ActionLink("Back", "Index", "SaleOrderHeader",null, new { @class = "btn btn-primary btn-sm col-md-offset-1 CurveBorder ActionLinkPadding" })
                    </div>
                </div>*@
            </div>
        </div>
    </div>
}

<script type="text/javascript">
    CustomSelectFunction($("#GodownId"), '/ComboHelpList/GetGodown', '/ComboHelpList/SetSingleGodown', ' ', false, 0);
    CustomSelectFunction($("#JobReceiveHeaderIds"), '/WeavingReceiveImportFromBranchToMain/GetJobReceive', '/WeavingReceiveImportFromBranchToMain/SetJobReceive', ' ', true, 0, 0);
    CustomSelectFunction($("#JobReceiveById"), '/ComboHelpList/GetEmployees', '/ComboHelpList/SetSingleEmployee', ' ', false, 0);
 </script>